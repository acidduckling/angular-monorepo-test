{
  "name": "monorepo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-p start:json start:m start:l",
    "start:l": "ng serve frl-lodge --port 60001 -o",
    "start:m": "ng serve frl-manage --port 60002 -o",
    "start:json": "json-server ./mock-data/db.json --routes ./mock-data/routes.json --port 60004",
    "build": "run-p build:m build:l",
    "build:c": "ng build frl-common && cd dist/frl-common && npm pack",
    "build:m": "ng build frl-manage --prod",
    "build:l": "ng build frl-lodge --prod",
    "test": "run-p test:c test:l test:m",
    "test:c": "ng test frl-common",
    "test:l": "ng test frl-lodge",
    "test:m": "ng test frl-manage",
    "dtest:c": "ng test frl-common --karma-config=projects/frl-common/karma.conf.dev.js",
    "dtest:l": "ng test frl-lodge --karma-config=projects/frl-lodge/karma.conf.dev.js",
    "dtest:m": "ng test frl-manage --karma-config=projects/frl-manage/karma.conf.dev.js",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "ng-packagr": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "rxjs": "~6.5.2",
    "zone.js": "^0.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.801.2",
    "@angular-devkit/build-ng-packagr": "~0.801.2",
    "@angular/cli": "^8.2.0",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.0",
    "@types/jasmine": "~3.3.16",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~12.6.8",
    "codelyzer": "^5.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.15.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.0.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "local-web-server": "^3.0.7",
    "protractor": "~5.4.2",
    "ts-node": "~8.3.0",
    "tsickle": ">=0.36.0",
    "tslib": "^1.10.0",
    "tslint": "~5.18.0",
    "typescript": "3.5"
  }
}
