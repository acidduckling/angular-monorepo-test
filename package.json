{
  "name": "monorepo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:m": "ng serve frl-manage -o",
    "start:l": "ng serve frl-lodge -o",
    "start:pl": "node_modules/.bin/ws -d dist/frl-lodge -o",
    "start:pm": "node_modules/.bin/ws -d dist/frl-manage -o",
    "start:cover": "node node_modules/local-web-server/bin/cli.js -d coverage/ -o",
    "build:c": "ng build frl-common && cd dist/frl-common && npm pack",
    "build:m": "ng build frl-manage --prod",
    "build:l": "ng build frl-lodge --prod",
    "build:all": "npm run build:m && npm run build:l",
    "test:c": "ng test frl-common --code-coverage",
    "test:l": "ng test frl-lodge --code-coverage",
    "test:m": "ng test frl-manage --code-coverage",
    "test:all": "npm run test:c && npm run test:l && npm run test:m && node scripts/merge-reports.js",
    "dtest:c": "ng test frl-common --karma-config=projects/frl-common/karma.conf.dev.js",
    "dtest:l": "ng test frl-lodge --karma-config=projects/frl-lodge/karma.conf.dev.js",
    "dtest:m": "ng test frl-manage --karma-config=projects/frl-manage/karma.conf.dev.js",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "istanbul-combine": "^0.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "local-web-server": "^2.6.1",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
